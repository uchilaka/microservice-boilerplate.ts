{
  "name": "@uchilaka/microservice-boilerplate.ts",
  "version": "0.2.2",
  "description": "MicroService boilerplate, powered by Fastify, written in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "fastify",
    "tsc",
    "typescript",
    "nodejs",
    "node",
    "server",
    "microservice",
    "service"
  ],
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "jest --runInBand",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build": "yarn build:types && yarn build:tsc",
    "build:types": "tsc --emitDeclarationOnly",
    "build:tsc": "tsc -p tsconfig.json",
    "start": "PORT=3020 yarn build && node dist/server.js",
    "debug": "tsc -w -p tsconfig.json & nodemon -q -w dist dist/server.js",
    "prepublish": "yarn build:tsc"
  },
  "engines": {
    "npm": ">= 6.14.6",
    "node": ">= 12.18.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "fastify": "^3.4.1",
    "fastify-cors": "^4.1.0",
    "fastify-swagger": "^3.3.0",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.10.2",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "4.12.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-unused-imports": "^0.1.3",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4"
  },
  "peerDependencies": {
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "4.12.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "author": "Uche Chilaka <uche@lar.city>",
  "license": "MIT",
  "publisher": "LarCity LLC<support@larcity.com>",
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org/",
    "directory": "dist"
  },
  "repository": {
    "url": "https://github.com/uchilaka/microservice-boilerplate.ts.git",
    "type": "git"
  }
}
